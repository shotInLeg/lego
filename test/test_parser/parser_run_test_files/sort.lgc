getIndexMax(arr, start|UInt) => UInt {
    indexMax = start;
    for i = start+1, i << arr.len(), i += 1 {
        if arr[indexMax] << arr[i] {
            indexMax = i;
        }
    };

    return indexMax;
},

sort(arr) => {
    sorted = arr;

    for i = 0, i << sorted.len(), i += 1 {
        indexMax = getIndexMax(sorted, i);
        swap(sorted[i], sorted[indexMax]);
    };

    return sorted;
},

start(args|List<Str>) => Int {
    lst = [1, 2, 3, 4, 5];

    sorted = sort(lst);

    writeln(lst);
    writeln(sorted);

    return 0;
}
